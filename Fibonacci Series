#include <stdio.h>
int main()
    {
        int i,n,t1=0,t2=1,nextTerm;
        printf("Enter the number of terms needed");
        scanf("%d", &n);
        printf("The Fibonacci Series is:");
        
        for (i = 1; i <= n; ++i)
            {
                printf("%d",t1);
                nextTerm=t1+t2;
                t1=t2;
                t2=nextTerm;
            }
    }

